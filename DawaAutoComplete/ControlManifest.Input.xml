<?xml version="1.0" encoding="utf-8" ?>
<manifest>
  <control namespace="DawaAutoComplete" constructor="DawaAutoComplete" version="0.0.2" display-name-key="DawaAutoComplete" description-key="DawaAutoComplete description" control-type="standard">
